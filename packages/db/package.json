{
    "name": "@kodetama/db",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./schema": "./dist/schema.js"
    },
    "scripts": {
        "build": "tsc",
        "generate": "dotenv -e ../../.env -- drizzle-kit generate",
        "migrate": "dotenv -e ../../.env -- drizzle-kit migrate",
        "push": "dotenv -e ../../.env -- drizzle-kit push",
        "studio": "dotenv -e ../../.env -- drizzle-kit studio",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "drizzle-orm": "^0.33.0",
        "postgres": "^3.4.4"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "dotenv-cli": "^7.4.2",
        "drizzle-kit": "^0.24.0",
        "typescript": "^5.3.0"
    }
}